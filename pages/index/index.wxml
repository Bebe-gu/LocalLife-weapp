<swiper class="slides" indicator-dots="{{true}}" autoplay="{{true}}" interval="{{5000}}" duration="{{500}}" circular="{{true}}" indicator-active-color="#ccc">
  <!-- <swiper-item wx:for="{{slides}}">
    <image class="swiper-image" src="{{item.image}}" mode="aspectFill"  wx:key="{{item.id}}"/>
  </swiper-item> -->
  <swiper-item>
    <image class="swiper-image" src="/assets/images/banner-01.png" mode="aspectFill" />
  </swiper-item>
  <swiper-item>
    <image class="swiper-image" src="/assets/images/banner-02.gif" mode="aspectFill" />
  </swiper-item>
  <swiper-item>
    <image class="swiper-image" src="/assets/images/banner-03.jpg" mode="aspectFill" />
  </swiper-item>
  <swiper-item>
    <image class="swiper-image" src="/assets/images/banner-04.jpg" mode="aspectFill" />
  </swiper-item>
</swiper>
<view class="grids">
  <navigator url="/pages/list/list?cat={{item.id}}" class="item" wx:for="{{categories}}" wx:key="{{item.id}}">
    <image src="{{item.icon}}" mode="aspectFill"></image>
    <text>{{item.name}}</text>
  </navigator>
</view>
<view class="nav">
  <view class="item">
    <image src="/assets/images/link-01.png"></image>
  </view>
  <view class="item">
    <image src="/assets/images/link-02.png"></image>
  </view>
</view>